import java.util.Random;

public class Main {

    public static void printMessage(String str, int num) {
        System.out.println("Hello JavaSE 11");
    }

    public static void multiply(int a, int b) {
        int result = a * b;
        System.out.println("掛け算結果: " + result);
    }

    public static void printArray(int[] arr) {
        System.out.println("配列の要素:");
        for (int val : arr) {
            System.out.println(val);
        }
    }

    public static void add(double a, double b) {
        double result = a + b;
        System.out.println("小数の和: " + result);
    }

    public static int[] generateRandomNumbers(int count) {
        Random rand = new Random();
        int[] arr = new int[count];
        for (int i = 0; i < count; i++) {
            int num;
            do {
                num = rand.nextInt(101); // 0～100
            } while (num == 0); // 0はNG
            arr[i] = num;
            System.out.println("生成された値: " + num);
        }
        return arr;
    }

    public static double printAverage(int[] arr) {
        double sum = 0;
        for (int val : arr) {
            sum += val;
        }
        double avg = sum / arr.length;
        System.out.println("平均値: " + avg);
        return avg;
    }


    public static boolean isAverageAbove50(double avg) {
        boolean result = avg >= 50;
        System.out.println("50以上か？ → " + result);
        return result;
    }

    public static void main(String[] args) {

        printMessage("Java", 11);


        multiply(5, 7);


        int[] sampleArray = {1, 2, 3, 4, 5};
        printArray(sampleArray);


        add(3.5, 2.7);

  
        int[] randomNumbers = generateRandomNumbers(5);


        double avg = printAverage(randomNumbers);


        isAverageAbove50(avg);
    }
}
